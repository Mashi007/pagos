{
  "name": "rapicredit-frontend",
  "private": true,
  "version": "1.0.1",
  "type": "module",
  "engines": {
    "node": ">=20.19.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "setup": "node setup.js",
    "dev": "vite",
    "build": "tsc --noEmit --skipLibCheck && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:unit": "vitest run tests/unit/",
    "test:integration": "vitest run tests/integration/",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "analyze:size": "npm run build && npx vite-bundle-analyzer dist --mode size",
    "analyze:gzip": "npm run build && npx vite-bundle-analyzer dist --mode gzip",
    "accessibility": "npm run build && npx @axe-core/cli https://rapicredit.onrender.com",
    "accessibility:local": "npm run build && npm run preview & sleep 5 && npx @axe-core/cli http://localhost:4173",
    "preview": "vite preview --host 0.0.0.0 --port $PORT",
    "start": "npm run setup && npm run dev",
    "serve": "serve -s dist -l $PORT",
    "render-build": "npm install && npm run build",
    "render-start": "node server.js"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@hookform/resolvers": "^3.3.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-select": "^2.0.0",
    "@tanstack/react-query": "^5.90.9",
    "@vitejs/plugin-react": "^4.1.1",
    "autoprefixer": "^10.4.22",
    "axios": "^1.13.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "compression": "^1.8.1",
    "date-fns": "^2.30.0",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "framer-motion": "^10.16.5",
    "http-proxy-middleware": "^3.0.3",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.294.0",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.66.0",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.30.2",
    "recharts": "^2.8.0",
    "serve": "^14.2.1",
    "sonner": "^1.2.4",
    "tailwind-merge": "^2.0.0",
    "tailwindcss": "^3.3.6",
    "vite": "^6.4.1",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@axe-core/cli": "^4.8.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitest/coverage-v8": "^4.0.9",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "jsdom": "^23.0.0",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.0",
    "typescript": "^5.2.2",
    "vite-bundle-analyzer": "^0.7.0",
    "vitest": "^4.0.9"
  }
}
